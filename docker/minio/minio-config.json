{
   "version": "36",
   "api": {
      "requests_max": 16,
      "requests_deadline": "10m",
      "cors": {
         "allow_origin": ["https://boilerplate-nextjs-3afb6505.christiangotthardt.de", "http://localhost:3000", "http://localhost:3034"],
         "allow_methods": ["GET", "PUT", "POST", "DELETE", "OPTIONS"],
         "allow_headers": ["*", "content-type", "x-amz-date", "x-amz-algorithm", "x-amz-credential", "x-amz-signature", "authorization"],
         "expose_headers": ["ETag", "Content-Length", "Content-Type", "Content-Disposition", "X-Amz-*"],
         "max_age_seconds": 3600
      }
   },
   "region": "us-east-1",
   "worm": "off",
   "cache": { "drives": [], "expiry": 90, "quota": 80, "exclude": [] },
   "compression": { "enable": true, "extensions": [".txt", ".log", ".csv", ".json"], "mime_types": ["text/*", "application/json"] },
   "etcd": { "endpoints": [], "path_prefix": "", "coredns_path": "/skydns", "client_cert": "", "client_cert_key": "" },
   "browser": { "enabled": true },
   "notify": {
      "webhook": {
         "1": { "enable": true, "endpoint": "http://nextjs:3000/api/minio-webhook", "auth_token": "", "client_cert": "", "client_key": "" }
      }
   }
}
