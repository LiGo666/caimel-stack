#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/nextjs/node_modules/.pnpm/ultracite@5.3.3_@types+node@20.19.15_jiti@1.21.7_typescript@5.9.2_yaml@2.8.1/node_modules/ultracite/dist/node_modules:/nextjs/node_modules/.pnpm/ultracite@5.3.3_@types+node@20.19.15_jiti@1.21.7_typescript@5.9.2_yaml@2.8.1/node_modules/ultracite/node_modules:/nextjs/node_modules/.pnpm/ultracite@5.3.3_@types+node@20.19.15_jiti@1.21.7_typescript@5.9.2_yaml@2.8.1/node_modules:/nextjs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/nextjs/node_modules/.pnpm/ultracite@5.3.3_@types+node@20.19.15_jiti@1.21.7_typescript@5.9.2_yaml@2.8.1/node_modules/ultracite/dist/node_modules:/nextjs/node_modules/.pnpm/ultracite@5.3.3_@types+node@20.19.15_jiti@1.21.7_typescript@5.9.2_yaml@2.8.1/node_modules/ultracite/node_modules:/nextjs/node_modules/.pnpm/ultracite@5.3.3_@types+node@20.19.15_jiti@1.21.7_typescript@5.9.2_yaml@2.8.1/node_modules:/nextjs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ultracite/dist/index.js" "$@"
else
  exec node  "$basedir/../ultracite/dist/index.js" "$@"
fi
